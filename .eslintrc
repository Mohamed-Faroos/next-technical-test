{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
        "no-console" : "warn",
        "semi":"error",
        "quotes" : ["warn", "double"],
        "@typescript-eslint/no-explicit-any" : "off",
        "@typescript-eslint/no-empty-function": "off"
    }
}